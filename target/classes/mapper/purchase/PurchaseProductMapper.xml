  <!--根据采购订单编号查询采购产品-->
  <select id="findPurchaseProduct"  resultMap="BaseResultMap">
    select * from purchase_product where  purchaseid =#{purchaseid}
  </select>

  <select id="findPurchaseProductNotStock" resultMap="BaseResultMapTwo">
    select ord.*,pro.* from purchase_product pro
    left join purchase_orders ord on ord.id = pro.purchaseid
     where num > stockover
     order by pro.id desc
  </select>

  <select id="findPurchaseProductByPronum" resultMap="BaseResultMap">
    select * from purchase_product where  purchaseid =#{purchaseid} and proorders =#{pronum} limit 0,1
  </select>


  <select id="findPurchaseProductNew" resultMap="BaseResultMap">

    select pro.proorders,pro.proname,pro.num,pro.remarks,ords.orders "purchaseOrders.orders" ,ords.seeorders "purchaseOrders.seeorders",ords.createdate "purchaseOrders.createdate"  from purchase_product pro
    left join purchase_orders ords on ords.id = pro.purchaseid
    <where>

      <if test="sisnum != '' and sisnum != null ">
        and  pro.proorders &gt;= #{sisnum}
      </if>
      <if test="eisnum != '' and eisnum != null ">
        and  pro.proorders &lt;= #{eisnum}
      </if>
      <if test="sproname != '' and sproname != null ">
        and  pro.proname &gt;= #{sproname}
      </if>
      <if test="eproname != '' and eproname != null ">
        and  pro.proname &lt;= #{eproname}
      </if>
      <!--<if test="stype != '' and stype != null ">
        and  productorder &gt;= #{stype}
      </if>
      <if test="etype != '' and etype != null ">
        and  productorder &lt;= #{etype}
      </if>-->

      <if test="sdate != null and sdate !=''    ">

        AND  DATE_FORMAT(ords.createdate,'%Y-%m-%d') &gt;= #{sdate}

      </if>

      <if test="edate != null and edate !=''   ">

        AND   DATE_FORMAT(ords.createdate,'%Y-%m-%d') &lt;= #{edate}

      </if>

    </where>
	order by ords.createdate desc
  </select>

  <select id="findPurchaseProductPriceSUM" resultType="java.lang.Double">
    select SUM(pp.totalprice) from purchase_product pp where pp.purchaseid = #{purchaseid};
  </select>

  <select id="findPurchaseProductAndProductNameAndStartTimeAndEndTime" resultMap="BaseResultMap">
    select po.createdate "purchaseOrders.createdate",po.orders "purchaseOrders.orders",po.suppliername "purchaseOrders.suppliername",pp.num,pp.price,po.ptax "purchaseOrders.ptax",pp.remarks from purchase_product pp ,purchase_orders po

    <where>

    and  pp.purchaseid =  po.id

    and pp.proorders = #{productName}

      <if test="startTime != null and startTime != ''" >
        and str_to_date(po.createdate, '%Y-%m-%d') &gt;= str_to_date(#{startTime}, '%Y-%m-%d')
      </if>
      <if test="endTime != null and endTime != ''" >
        and str_to_date(po.createdate, '%Y-%m-%d') &lt;= str_to_date(#{endTime}, '%Y-%m-%d')
      </if>

    </where>

      order by po.createdate desc
  </select>


  <select id="findPurchaseProductByOrdersAndProductName" resultMap="BaseResultMapTwo">

    select pp.*,po.* from purchase_product pp , purchase_orders po where pp.purchaseid = po.id  and po.orders = #{purchaseOrders} and pp.proorders = #{productOrders}

    </select>

