	<queryString>
		<![CDATA[/* 销货明细报表 -- 日期别 */
select g.create_date,g.sale_no,g.customer_no_str,g.business_leader_str,g.warehouse_out_str,g.currency,p.num*p.price 'sale_money',(
        CASE g.tax
        WHEN '外加' THEN
            p.price*p.num*(g.tax_rate)
        ELSE
            0
        END
			)  'tax_money',(
        CASE g.tax
        WHEN '外加' THEN
            p.price*p.num*(g.tax_rate+1)
        ELSE
            p.price*p.num
        END
			)  'calc_money',g.tax,p.product_no,p.product_name,p.num,p.unit,p.price,p.num*p.price 'money' from sale_goods g RIGHT JOIN sale_goods_product p ON g.id=p.sale_goods_id
			where 1 = 1
			and if($P{createDateBen} ='' ,1=1, $P{createDateBen}<=g.create_date) and if($P{createDateEnd} ='' ,1=1,$P{createDateEnd}>=g.create_date)
			and if($P{saleNoBen} ='' ,1=1, $P{saleNoBen}<=g.sale_no) and if($P{saleNoEnd} ='' ,1=1,$P{saleNoEnd}>=g.sale_no)
			and if($P{customerNoBen} ='' ,1=1, $P{customerNoBen}<=g.customer_no) and if($P{customerNoEnd} ='' ,1=1,$P{customerNoEnd}>=g.customer_no)
			and if($P{businessLeaderBen} ='' ,1=1, $P{businessLeaderBen}<=g.business_leader_str) and if($P{businessLeaderEnd} ='' ,1=1,$P{businessLeaderEnd}>=g.business_leader_str)
			and if($P{warehouseBen} ='' ,1=1, $P{warehouseBen}<=g.warehouse_out_str) and if($P{warehouseEnd} ='' ,1=1,$P{warehouseEnd}>=g.warehouse_out_str)
			and if($P{productNoBen} ='' ,1=1, $P{productNoBen}<=p.product_no) and if($P{productNoEnd} ='' ,1=1,$P{productNoEnd}>=p.product_no)
			and if($P{productCategoryBen} ='' ,1=1, $P{productCategoryBen}<=p.category) and if($P{productCategoryEnd} ='' ,1=1,$P{productCategoryEnd}>=p.category)
			ORDER BY g.create_date asc,g.sale_no asc ;]]>
	</queryString>
